{
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.portadesign.cz"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"portabucket/pimcore": "4.0.0-p3579",
		"zendframework/zendframework1": "~1.12",
		"symfony/console": "~2.7",
		"symfony/finder": "~2.7",
		"symfony/var-dumper": "~2.7",
		"symfony/css-selector": "~2.7",
		"filp/whoops": "~1",
		"tijsverkoyen/css-to-inline-styles": "~1.5",
		"hybridauth/hybridauth": "~2",
		"google/apiclient": "~1",
		"oyejorge/less.php": "~1.7",
		"geoip2/geoip2": "~2"
	},
	"extra": {
		"installer-paths": {
			"pimcore": ["portabucket/pimcore"]
		}
	},
	"autoload": {
		"psr-4": {
			"Pimcore\\": "pimcore/lib/Pimcore",
			"Website\\Controller\\": "website/controllers",
			"Website\\Model\\": "website/models",
			"Website\\Form\\": "website/forms",
			"Website\\Tool\\": "website/tools",
			"Website\\View\\Helper\\": "website/views/helpers"
		}
	},
	"scripts": {
		"post-install-cmd": "Pimcore\\Composer::postInstall",
		"post-update-cmd": "Pimcore\\Composer::postUpdate"
	}
}
